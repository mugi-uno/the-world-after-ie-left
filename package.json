{
  "private": true,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "generate:compat": "ts-node -T ./app/lib/compat-data/scripts/generate.ts"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "^5.2.13",
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "@remix-run/vercel": "^1.6.0",
    "@vercel/node": "^1.15.2",
    "clsx": "^1.1.1",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "@tsconfig/strictest": "^1.0.1",
    "@types/node": "^17.0.43",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/semver": "^7.3.10",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
